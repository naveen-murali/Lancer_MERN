{
  "name": "lancer",
  "version": "2.0.0",
  "description": "A freelancers platform",
  "main": "server.js",
  "repository": "https://github.com/naveen-murali/Lancer.git",
  "author": "Naveen Murali <naveenmurali1211@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/server.js",
    "start:dev": "cross-env NODE_ENV=development node build/server.js",
    "build": "tsc && tsc-alias",
    "build:start": "yarn build && yarn start",
    "dev": "cross-env NODE_ENV=development ts-node-dev server.ts",
    
    "dev:docker": "docker-compose -f docker-compose.yml -f docker-compose-dev.yml up",
    "dev:docker:down": "docker-compose -f docker-compose.yml -f docker-compose-dev.yml down",
    "dev:docker:build": "yarn dev:docker --build",
    "start:docker": "docker-compose up -d"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^7.2.0",
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.28.1",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "firebase-admin": "^10.0.2",
    "google-auth-library": "^7.14.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "mongoose": "^6.2.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nanoid": "^3.3.1",
    "nodemailer": "^6.7.3",
    "paypal-rest-sdk": "^1.8.1",
    "redis": "^4.1.0",
    "scrape-it": "^5.3.2",
    "socket.io": "^4.5.0",
    "twilio": "^3.75.1",
    "zod": "^3.14.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/nodemailer": "^6.4.4",
    "@types/paypal-rest-sdk": "^1.7.6",
    "@types/socket.io": "^3.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.6.4",
    "typescript": "^4.6.2"
  }
}